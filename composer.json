{
    "name": "cmd-wrapper/wrapper-contracts",
    "description": "Wrapper Contracts",
    "type": "library",
    "require-dev": {
        "php": ">= 7.4",
        "artarts36/merge-request-linter": "^0.1.3",
        "artarts36/php-cs-fixer-good-fixers": "^0.1.2",
        "guzzlehttp/guzzle": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CmdWrapper\\Contracts\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ArtARTs36",
            "email": "temicska99@mail.ru"
        }
    ],
    "scripts": {
        "lint": [
            "php-cs-fixer fix --dry-run --verbose --diff --ansi --allow-risky=yes"
        ],
        "mr-lint": [
            "./vendor/bin/mr-linter lint"
        ]
    }
}
